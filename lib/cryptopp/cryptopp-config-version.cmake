set (CRYPTOPP_VERSION_MAJOR 5)
set (CRYPTOPP_VERSION_MINOR 6)

set (PACKAGE_VERSION_COUNT 3)
set (PACKAGE_VERSION
  5.6.1)

# Compatible only if the requested and the actual major version are equal
if (PACKAGE_FIND_VERSION_MAJOR EQUAL CRYPTOPP_VERSION_MAJOR)
  set (PACKAGE_VERSION_COMPATIBLE TRUE)
else (PACKAGE_FIND_VERSION_MAJOR EQUAL CRYPTOPP_VERSION_MAJOR)
  set (PACKAGE_VERSION_COMPATIBLE FALSE)
endif (PACKAGE_FIND_VERSION_MAJOR EQUAL CRYPTOPP_VERSION_MAJOR)

if (PACKAGE_FIND_VERSION_MAJOR EQUAL CRYPTOPP_VERSION_MAJOR AND
    PACKAGE_FIND_VERSION_MINOR EQUAL CRYPTOPP_VERSION_MINOR)
  set (PACKAGE_VERSION_EXACT TRUE)
else ()
  set (PACKAGE_VERSION_EXACT FALSE)
endif ()
